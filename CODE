--------------CODE----------------
void setup()
{
  Serial.begin(9600);
  pinMode(7, INPUT);
  pinMode(6, INPUT);
}

void loop()
{
  Serial.print("PULLDOWN : ");
  Serial.print(digitalRead(7));
  Serial.print("                  ");
  Serial.print("PULLUP : ");
  Serial.println(digitalRead(6));
  delay(1000); // Wait for 1000 millisecond(s)
}
